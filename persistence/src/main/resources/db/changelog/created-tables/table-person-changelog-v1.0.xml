<databaseChangeLog
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
         https://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-4.1.xsd">

    <changeSet id="table-social-media-changelog-v1.0.xml" author="cagataysar">
        <createTable tableName="person" schemaName="good_books">
            <column name="person_id" type="bigint" autoIncrement="true">
                <constraints primaryKey="true" nullable="false"/>
            </column>
            <column name="person_name" type="varchar(255)"/>
            <column name="person_surname" type="varchar(255)"/>
            <column name="person_biography" type="varchar(255)"/>
            <column name="person_photo" type="varchar(255)"/>
        </createTable>
    </changeSet>
</databaseChangeLog>