<?xml version="1.0" encoding="UTF-8"?>
<databaseChangeLog
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
         https://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-4.1.xsd">

    <changeSet id="table-account-changelog-v1.0.xml" author="suleymankisir">
        <createTable tableName="account" schemaName="good_books">
            <column name="account_id" type="bigint">
                <constraints primaryKey="true" nullable="false"/>
            </column>
            <column name="account_username" type="varchar(255)"/>
            <column name="account_email" type="varchar(255)"/>
            <column name="account_password" type="varchar(255)"/>
            <column name="account_role" type="varchar(255)"/>

        </createTable>
    </changeSet>
</databaseChangeLog>